{
    "name" : "Ya-BOO",
    "description": "Trying to get paid the big bucks - extension.",
    "version" : "1.01",
    "manifest_version" : 3,
    "background": {
        "service_worker" : "background.js"
    },
    
   "permissions" : ["storage", "activeTab", "scripting", "declarativeNetRequest", "<all_urls>"],

   "action": {
    "default_popup": "popup.html",
    
    "default_icon": {
        "16": "/images/yahoo16.png",
        "32": "/images/yahoo32.png",
        "48": "/images/yahoo48.png",
        "128": "/images/yahoo128.png"
    },
    "icons": {
        "16": "/images/yahoo16.png",
        "32": "/images/yahoo32.png",
        "48": "/images/yahoo48.png",
        "128": "/images/yahoo128.png"
      }
  },
  "options_page": "options.html"
}


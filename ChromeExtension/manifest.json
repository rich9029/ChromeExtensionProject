{
    "name" : "Richard and Eli's Sick Chrome Extension",
    "description": "Trying to get paid the big bucks - extension.",
    "version" : "0.01",
    "manifest_version" : 3,
    "background": {
        "service_worker" : "background.js"
    },
    
   "permissions" : ["storage", "activeTab", "scripting", "webRequest", "webRequestBlocking", "<all_urls>"],

   "action": {
    "default_popup": "popup.html",
    
    "default_icon": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
    },
    "icons": {
        "16": "/images/get_started16.png",
        "32": "/images/get_started32.png",
        "48": "/images/get_started48.png",
        "128": "/images/get_started128.png"
      }
  },
  "options_page": "options.html"
}

